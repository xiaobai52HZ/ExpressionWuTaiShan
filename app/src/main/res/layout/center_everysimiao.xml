<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:orientation="vertical"    android:layout_width="match_parent" android:layout_height="230dp">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="115dp"        android:orientation="horizontal">        <LinearLayout            android:orientation="vertical"            android:layout_weight="1"            android:layout_width="0dp"            android:layout_height="match_parent">            <android.support.design.widget.FloatingActionButton                android:id="@+id/simple_text_every"                android:layout_width="60dp"                android:layout_height="60dp"                android:layout_marginTop="15dp"                android:layout_gravity="center"                android:src="@drawable/icon_jianjie"                app:backgroundTint="?attr/colorControlActivated" />            <TextView                android:layout_marginTop="10dp"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:gravity="center"                android:text="简介" />        </LinearLayout>        <LinearLayout            android:orientation="vertical"            android:layout_weight="1"            android:layout_width="0dp"            android:layout_height="match_parent">            <android.support.design.widget.FloatingActionButton                android:id="@+id/couplets_every"                android:layout_width="60dp"                android:layout_height="60dp"                android:layout_marginTop="15dp"                android:layout_gravity="center"                android:src="@drawable/icon_couplets"                app:backgroundTint="@android:color/holo_green_light" />            <TextView                android:layout_width="match_parent"                android:layout_marginTop="10dp"                android:layout_height="wrap_content"                android:gravity="center"                android:text="楹联文化" />        </LinearLayout>        <LinearLayout            android:orientation="vertical"            android:layout_weight="1"            android:layout_width="0dp"            android:layout_height="match_parent">            <android.support.design.widget.FloatingActionButton                android:id="@+id/navigation_every"                android:layout_width="60dp"                android:layout_height="60dp"                android:layout_marginTop="15dp"                android:layout_gravity="center"                android:src="@drawable/navigation"                app:backgroundTint="@android:color/holo_orange_dark" />            <TextView                android:layout_width="match_parent"                android:layout_marginTop="10dp"                android:layout_height="wrap_content"                android:gravity="center"                android:text="导航解说" />        </LinearLayout>    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="115dp"        android:orientation="horizontal">        <LinearLayout            android:orientation="vertical"            android:layout_weight="1"            android:layout_width="0dp"            android:layout_height="match_parent">            <android.support.design.widget.FloatingActionButton                android:id="@+id/stroy_every"                android:layout_width="60dp"                android:layout_height="60dp"                android:layout_marginTop="15dp"                android:layout_gravity="center"                android:src="@drawable/icon_stroy"                app:backgroundTint="?attr/actionModeSplitBackground" />            <TextView                android:layout_marginTop="10dp"                android:gravity="center"                android:text="传说故事"                android:layout_width="match_parent"                android:layout_height="wrap_content" />        </LinearLayout>        <LinearLayout            android:orientation="vertical"            android:layout_weight="1"            android:layout_width="0dp"            android:layout_height="match_parent">            <android.support.design.widget.FloatingActionButton                android:id="@+id/vr_every"                android:layout_width="60dp"                android:layout_height="60dp"                android:layout_marginTop="15dp"                android:layout_gravity="center"                android:src="@drawable/icon_vr"                app:backgroundTint="@color/rouhelan" />            <TextView                android:layout_marginTop="10dp"                android:gravity="center"                android:text="VR"                android:layout_width="match_parent"                android:layout_height="wrap_content" />        </LinearLayout>        <LinearLayout            android:orientation="vertical"            android:layout_weight="1"            android:layout_width="0dp"            android:layout_height="match_parent">            <android.support.design.widget.FloatingActionButton                android:id="@+id/yunyinjieshuo_every"                android:layout_width="60dp"                android:layout_height="60dp"                android:layout_marginTop="15dp"                android:layout_gravity="center"                android:src="@drawable/simiao_zonglan"                app:backgroundTint="@android:color/holo_purple" />            <TextView                android:layout_marginTop="10dp"                android:gravity="center"                android:text="大殿总览"                android:layout_width="match_parent"                android:layout_height="wrap_content" />        </LinearLayout>    </LinearLayout></LinearLayout>