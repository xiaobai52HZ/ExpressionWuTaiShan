<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    tools:context=".Stroy.Stroy_Activity">    <android.support.design.widget.CoordinatorLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <android.support.design.widget.AppBarLayout            android:layout_width="match_parent"            android:layout_height="wrap_content">            <android.support.v7.widget.Toolbar                android:id="@+id/stroy_toolbar"                android:layout_width="match_parent"                android:layout_height="35dp"                android:background="@color/rouhelan"                app:layout_scrollFlags="scroll|enterAlways|snap">            </android.support.v7.widget.Toolbar>        </android.support.design.widget.AppBarLayout>        <android.support.v4.widget.NestedScrollView            android:layout_width="match_parent"            android:layout_height="match_parent"            app:layout_behavior="@string/appbar_scrolling_view_behavior">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:orientation="vertical">                <TextView                    android:textIsSelectable="true"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:padding="8dp"                    android:text="五台山简介"                    android:textSize="35dp" />                <TextView                    android:textIsSelectable="true"                    android:id="@+id/text_set1"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:lineSpacingExtra="5dp"                    android:padding="7dp"                    android:textSize="15dp" />                <ImageView                    android:id="@+id/img_set1"                    android:layout_width="match_parent"                    android:layout_height="170dp"                    android:layout_margin="13dp"                    android:scaleType="centerCrop" />                <TextView                    android:textIsSelectable="true"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:padding="8dp"                    android:text="地质地理"                    android:textSize="30dp" />                <TextView                    android:textIsSelectable="true"                    android:id="@+id/text_set2"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:lineSpacingExtra="5dp"                    android:padding="7dp"                    android:textSize="15dp" />                <ImageView                    android:id="@+id/img_set2"                    android:layout_width="match_parent"                    android:layout_height="170dp"                    android:layout_margin="13dp"                    android:scaleType="centerCrop" />                <TextView                    android:textIsSelectable="true"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:padding="8dp"                    android:text="气候特点"                    android:textSize="30dp" />                <TextView                    android:textIsSelectable="true"                    android:id="@+id/text_set3"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:lineSpacingExtra="5dp"                    android:padding="7dp"                    android:textSize="15dp" />                <ImageView                    android:id="@+id/img_set3"                    android:layout_width="match_parent"                    android:layout_height="170dp"                    android:layout_margin="13dp"                    android:scaleType="centerCrop" />                <TextView                    android:textIsSelectable="true"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:padding="8dp"                    android:text="旖旎风光"                    android:textSize="30dp" />                <TextView                    android:textIsSelectable="true"                    android:id="@+id/text_set4"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:lineSpacingExtra="5dp"                    android:padding="7dp"                    android:textSize="15dp" />                <ImageView                    android:layout_width="match_parent"                    android:layout_height="170dp"                    android:layout_margin="13dp"                    android:scaleType="centerCrop" />            </LinearLayout>        </android.support.v4.widget.NestedScrollView>    </android.support.design.widget.CoordinatorLayout></LinearLayout>